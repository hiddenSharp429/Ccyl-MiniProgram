  .container{
         position: relative;
  }
  
  .container_content{
         position: relative;
         min-height: 100vh;
         box-sizing: border-box;
         padding-bottom: 60rpx;
         padding-top: 0rpx;
  }
  
  .tabar{
         width: 100%;
         text-align: center;
         letter-spacing: 4rpx;
         position: absolute;
         bottom: 0;
  
  }

  .name{
         font-size: 17px;
         font-weight: 200;
         color: #174099;
  }
